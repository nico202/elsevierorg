	pages = {245--258},
	file = {Effectiveness of virtual reality rehabilitation for children and adolescents with cerebral palsy\: an updated evidence-based systematic review:/home/nbalzarotti/Zotero/storage/4CLDFPEZ/j.physio.2016.08.004.pdf.pdf:application/pdf;ScienceDirect Snapshot:/home/nbalzarotti/Zotero/storage/7VYYSKXX/S0031940616300645.html:text/html},
