	keywords = {Stroke, Stroke Rehabilitation, Brain, Humans, Motor Activity, Recovery of Function, Female, Male, Middle Aged, Adult, Chronic Disease, Feedback, Sensory, Treatment Outcome, Electric Stimulation Therapy, Paralysis, Hand, Brain-Computer Interfaces, Biomechanical Phenomena, -stroke rehabilitation functional electrical stimulation neurofeedback, Brain Waves, Brain-computer interface, Discriminant Analysis, Electroencephalography, Equipment Design, Linear Models, Pattern Recognition, Automated, Signal Processing, Computer-Assisted, Time Factors},
	pages = {E178--E184},
