	keywords = {Stroke, Humans, Brain Ischemia, Adolescent, Child, Female, Male, Child, Preschool, Age Factors, Age of Onset, Health Status, Longitudinal Studies, Neurologic Examination, Quality of Life},
	pages = {1027--1034},
	file = {Age at stroke onset influences the clinical outcome and health-related quality of life in pediatric ischemic stroke survivors:/home/nbalzarotti/Zotero/storage/Y8YIFTFU/dmcn.12870.pdf.pdf:application/pdf;Full Text:/home/nbalzarotti/Zotero/storage/4IJEDY8Y/Ghotra et al. - 2015 - Age at stroke onset influences the clinical outcom.pdf:application/pdf},
