	booktitle = {Proceedings of the {ACM} {Symposium} on {Virtual} {Reality} {Software} and {Technology}},
	publisher = {Association for Computing Machinery},
	author = {Slater, Mel and Linakis, Vasilis and Usoh, Martin and Kooper, Rob},
	month = jul,
	year = {1996},
	keywords = {immersion, presence, task performance, tri-dimensional chess, virtual environments, virtual reality},
