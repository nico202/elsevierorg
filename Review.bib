	pmid = {9808298},
	keywords = {Humans, Adult, Movement, Hand, Hand Strength, Mountaineering, Posture, Touch},
	pages = {253--259},
	file = {High postural constraints affect the organization of reaching and grasping movements:/home/nbalzarotti/Zotero/storage/FQUE3YTY/s002210050513.pdf.pdf:application/pdf},
}

