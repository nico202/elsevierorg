	keywords = {rehabilitation, Humans, Child, Aged, Adult, Chronic Disease, Exercise Therapy, Randomized Controlled Trials as Topic, Mountaineering, Disabled Children, Disabled Persons, exercise therapy, Low Back Pain, mountaineering, Multiple Sclerosis},
	pages = {Doc19},
